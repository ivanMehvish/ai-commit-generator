{
  "name": "ai-commit-gen",
  "displayName": "AI Commit Message Generator",
  "version": "0.0.1",
  "publisher": "your-name",
  "type": "module",
  "engines": {
    "vscode": "^1.80.0"
  },
  "activationEvents": [
    "onCommand:aiCommitGen.generateMessage"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "aiCommitGen.generateMessage",
        "title": "Generate Commit Message"
      }
    ]
  },
  "scripts": {
    "test-local": "cross-env AI_MODE=gemini node test_local.js ./test-repo"
  },
  "devDependencies": {
    "cross-env": "^10.0.0",
    "vscode": "^1.1.37"
  },
  "dependencies": {
    "dotenv": "^17.2.2",
    "google-auth-library": "^10.3.0",
    "node-fetch": "^3.3.2"
  }
}
